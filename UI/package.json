{
  "name": "project-generator",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel lint format:check",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "framer-motion": "^8.5.0",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.10.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "happy-dom": "^8.1.5",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "testing-library": "^0.0.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.28.1"
  }
}
